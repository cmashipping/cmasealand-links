{
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [{ "key": "Content-Type", "value": "application/json" }]
    }
  ],
  "redirects": [
    {
      "source": "/invite/:code",
      "destination": "https://YOUR_WEB_APP_HOST/#/login?invite=:code",
      "permanent": false
    },
    {
      "source": "/invite",
      "destination": "https://YOUR_WEB_APP_HOST/#/login",
      "permanent": false
    }
  ]
}
